<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <table border="1">
        <thead>
            <tr>
                <th>Name</th>
                <th>Username</th>
                <th>Email</th>
                <th>Gender</th>
                <th>Phone</th>
                <th>Address</th>
                <th colspan="2">Action</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(element => { %>
                <tr>
                    <td><%= element.first_name %></td>
                    <td><%= element.username%></td>
                    <td><%= element.email%></td>
                    <td><%= element.gender%></td>
                    <td><%= element.phone_number%></td>
                    <td><%= element.address%></td>
                    <td><a href="/users/edit/<%= element.id%>">Edit</a></td>
                    <td><a href="/shipments/<%= element.id %>/list">add Shipment</a></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>