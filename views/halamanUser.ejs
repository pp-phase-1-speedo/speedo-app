<%- include('./partials/head', {title: 'Add Destination' }) %>

    <table class="table">
        <thead class="table-dark">
            <tr>
                <th>Name</th>
                <th>Username</th>
                <th>Email</th>
                <th>Gender</th>
                <th>Phone</th>
                <th>Address</th>
                <th colspan="2">Action</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(element => { %>
                <tr>
                    <td class="table-success"><%= element.first_name %></td>
                    <td class="table-success"><%= element.username%></td>
                    <td class="table-success"><%= element.email%></td>
                    <td class="table-success"><%= element.gender%></td>
                    <td class="table-success"><%= element.phone_number%></td>
                    <td class="table-success"><%= element.address%></td>
                    <td class="table-success"><a href="/users/edit/<%= element.id%>">Edit</a></td>
                    <td class="table-success"><a href="/shipments/<%= element.id %>/list">add Shipment</a></td>
                </tr>
            <% }); %>
        </tbody>
    </table>

    <%- include('./partials/foot') %>